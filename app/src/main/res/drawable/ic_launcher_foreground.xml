<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
    
    <!-- Outer circle with cyan gradient -->
    <path android:pathData="M54,20 A34,34 0 1,1 54,88 A34,34 0 1,1 54,20">
        <aapt:attr name="android:fillColor">
            <gradient
                android:type="linear"
                android:startX="20"
                android:startY="20"
                android:endX="88"
                android:endY="88"
                android:startColor="#00D9FF"
                android:centerColor="#0099FF"
                android:endColor="#6B5FFF"/>
        </aapt:attr>
    </path>
    
    <!-- Middle wave circle with purple gradient -->
    <path android:pathData="M54,30 A24,24 0 1,1 54,78 A24,24 0 1,1 54,30"
        android:fillColor="#80FF006B"/>
    
    <!-- Inner circle with pink gradient -->
    <path android:pathData="M54,38 A16,16 0 1,1 54,70 A16,16 0 1,1 54,38">
        <aapt:attr name="android:fillColor">
            <gradient
                android:type="radial"
                android:centerX="54"
                android:centerY="54"
                android:gradientRadius="16"
                android:startColor="#FF00FF"
                android:endColor="#FF0080"/>
        </aapt:attr>
    </path>
    
    <!-- Accent wave shapes for dynamic effect -->
    <path android:pathData="M30,54 Q45,40 60,54 T90,54"
        android:strokeColor="#60FFFFFF"
        android:strokeWidth="3"
        android:fillColor="#00000000"/>
    
    <path android:pathData="M25,48 Q42,35 58,48 T88,48"
        android:strokeColor="#4000D9FF"
        android:strokeWidth="2"
        android:fillColor="#00000000"/>
</vector>